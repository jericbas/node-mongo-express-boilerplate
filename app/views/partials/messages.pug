if typeof errors !== "undefined" && errors.length > 0
   each err in  errors
    .alert.alert-warning.fade.show(role='alert') #{err.msg} 
else if error.length > 0
  .alert.alert-warning.fade.show(role='alert') #{error} 
else if error_msg.length > 0
  .alert.alert-danger.fade.show(role='alert') #{error_msg}
else if success_msg.length > 0
  .alert.alert-success.fade.show(role='alert') #{success_msg}
